#ifdef CAR_HPP
#define CAR_HPP
#include <string>
using namespace std;
class Car{
    public:
        int release;
        string customer;
        int speed;
        Car::Car() {
        ReleaseYear = 0;
        customer = "";
        speed = 0;
        std::cout << "Constructor is called!" << std::endl;
        }
        Car::~Car() {
        std::cout << "Destructor is called!" << std::endl;
        }
        int r()  {
        return release;
        }
        std::string c(){
            return customer;
        }
        int s(){
            return speed;
        }
        
        void acc()
        {
            speed=speed+10;
        }
        void brake()
        {
            speed=speed-10;
        }
};
#endif
